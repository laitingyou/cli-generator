{
  "name": "cli-generator",
  "version": "1.0.0",
  "description": "A framework that allows you to quickly generate command line interactive tools.",
  "main": "app.js",
  "keywords": [
    "serverless",
    "cli",
    "dida",
    "material"
  ],
  "bin": {
    "cli": "./app.js"
  },
  "files": [
    "cli",
    "README.md"
  ],
  "scripts": {
    "build:win": "pkg . -t win --out-path ./packages/window",
    "build:macos": "pkg . -t macos --out-path ./packages/macos",
    "build:linux": "pkg . -t linux --out-path ./packages/linux",
    "build:all": "npm run build:win && npm run build:macos && npm run build:linux"
  },
  "author": {
    "email": "laitingyou@outlook.com",
    "name": "laitingyou"
  },
  "pkg": {
    "scripts": [
      "./config/**/*.js",
      "./middleware/**/*.js",
      "./controller/**/*.js",
      "./service/**/*.js",
      "./utils/**/*.js",
      "./core/**/*.js"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "ali-oss": "^6.5.1",
    "chalk": "^3.0.0",
    "cli-color": "^2.0.0",
    "command-exists": "^1.2.8",
    "commander": "^4.1.1",
    "deepmerge": "^4.2.2",
    "download-git-repo": "^3.0.2",
    "figures": "^3.2.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.6",
    "ora": "^4.0.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "string-width": "^4.2.0",
    "user-home": "^2.0.0",
    "yargs-parser": "^18.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.14",
    "babel-loader": "^8.2.2",
    "lerna": "^3.20.2"
  }
}
